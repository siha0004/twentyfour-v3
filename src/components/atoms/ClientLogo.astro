---
import { Icon } from "astro-icon/components";
const { LogoName } = Astro.props;
---

<div class="w-[130px] h-[80px] lg:w-[160px] lg:h-[90px] px-4 py-3 border-2 border-border-secondary rounded-small flex items-center justify-center shrink-0 lg:px-6 lg:py-5">
  <Icon name={LogoName} class="w-[50px] h-auto text-text-primary lg:w-[70px]" />
</div>
